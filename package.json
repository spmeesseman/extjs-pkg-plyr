{
	"name": "extjs-pkg-plyr",
	"displayName": "ExtJS Package Wrapper for plyr",
	"description": "ExtJS Package Wrapper for plyr",
	"publisher": "spmeesseman",
	"version": "1.0.6",
	"license": "MIT",
	"homepage": "https://github.com/spmeesseman/extjs-pkg-plyr/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/spmeesseman/extjs-pkg-plyr.git"
	},
	"bugs": {
		"url": "https://github.com/spmeesseman/extjs-pkg-plyr/issues"
	},
	"dependencies": {
		"plyr": "^3.5.2"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^3.0.2",
		"@semantic-release/exec": "^3.3.0",
		"@semantic-release/git": "^7.0.8",
		"commitizen": "^3.0.7",
		"conventional-changelog-spm": "^1.1.3",
		"cz-conventional-changelog": "^2.1.0",
		"semantic-release": "^15.13.12"
	},
	"scripts": {
		"build": "npx sencha package build",
		"repo-release": "npx sencha package add build\\plyr\\plyr.pkg",
		"semantic-release": "semantic-release --no-ci",
		"semantic-release-dry": "semantic-release --dry-run"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"sencha": {
		"name": "plyr",
		"namespace": "Ext",
		"framework": "ext",
		"type": "code",
		"toolkit": "classic",
		"theme": "theme-neptune",
		"creator": "smeesseman",
		"summary": "Package wrapper for plyr",
		"detailedDescription": "Package wrapper for the plyr media player",
		"version": "3.5.2.0",
		"compatVersion": "3.5.2",
		"format": "1",
		"slicer": null,
		"output": "${framework.dir}/build/packages/${package.name}",
		"local": true,
		"resource": {
			"paths": ""
		},
		"resources": [
			{
				"path": "${package.dir}/../plyr/dist",
				"output": "base"
			}
		],
		"sass": {
			"namespace": "Ext",
			"etc": [
				"${package.dir}/sass/var"
			],
			"var": [
				"${package.dir}/sass/var"
			],
			"src": [
				"${package.dir}/sass/src",
				"${package.dir}/sass/src/main.scss"
			]
		},
		"classpath": [
			"${package.dir}/src"
		],
		"overrides": [
			"${package.dir}/overrides"
		],
		"requires": [
			"core"
		],
		"ignore": [
			"(^|/)CVS(/?$|/.*?$)"
		]
	}
}
